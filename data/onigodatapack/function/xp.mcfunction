#tick
    #ダッシュ中（スタミナ消費）
        execute as @a if predicate onigodatapack:dash run scoreboard players remove @s stamina 10
    #ダッシュ中じゃない(スタミナ増加)
        execute as @a if predicate onigodatapack:not_dash run scoreboard players add @s stamina 20


#effect
